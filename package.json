{
  "name": "vue-ts-startup-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vue-tsc -b && vite build",
    "ci:eslint": "eslint -f json src -o ./.ci/eslint.json",
    "dev": "vite --mode test",
    "dev:prod": "vite --mode prod",
    "lint": "eslint . && stylelint --allow-empty-input \"**/*.{css,less,scss}\"",
    "lint:fix": "prettier --write . && eslint --fix . && stylelint --allow-empty-input --fix \"**/*.{css,less,scss}\"",
    "preview": "vite preview",
    "commit": "pnpm lint:fix && git add . && czg",
    "clean": "rimraf node_modules dist"
  },
  "prettier": "prettier-config-ali",
  "stylelint": {
    "extends": [
      "stylelint-config-ali",
      "stylelint-prettier/recommended"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@unocss/reset": "^66.4.1",
    "alova": "^3.3.4",
    "elegant-router": "1.0.4-beta.10",
    "element-plus": "^2.10.5",
    "vue": "^3.5.17",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "czg": "^1.12.0",
    "eslint": "^9.32.0",
    "eslint-config-ali": "^16.3.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "prettier": "^3.6.2",
    "prettier-config-ali": "^1.3.4",
    "rimraf": "^6.0.1",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "stylelint": "^16.23.0",
    "stylelint-config-ali": "^2.1.2",
    "stylelint-prettier": "^5.0.3",
    "typescript": "~5.8.3",
    "unocss": "^66.4.1",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.0.4",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^2.2.12"
  }
}
