{
  "name": "vue-ts-startup-template",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/jiuyue1123/vue-ts-startup-template#readme",
  "bugs": {
    "url": "https://github.com/jiuyue1123/vue-ts-startup-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiuyue1123/vue-ts-startup-template.git"
  },
  "funding": "https://github.com/sponsors/jiuyue1123",
  "type": "module",
  "scripts": {
    "build": "vue-tsc -b && vite build",
    "ci:eslint": "eslint -f json src -o ./.ci/eslint.json",
    "clean": "rimraf node_modules dist",
    "commit": "pnpm lint:fix && git add . && czg",
    "dev": "vite --mode test",
    "dev:prod": "vite --mode prod",
    "lint": "eslint . && stylelint --allow-empty-input \"**/*.{css,less,scss}\"",
    "lint:fix": "prettier --write . && eslint --fix . && stylelint --allow-empty-input --fix \"**/*.{css,less,scss}\"",
    "prepare": "husky",
    "preview": "vite preview"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{cjs,cts,js,jsx,mjs,mts,ts,tsx,vue}": "eslint --fix",
    "*.{css,less,scss}": "stylelint --fix",
    "*.{cjs,css,cts,html,js,json,jsx,less,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}": "prettier --write"
  },
  "prettier": "prettier-config-ali",
  "stylelint": {
    "extends": [
      "stylelint-config-ali",
      "stylelint-prettier/recommended"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@unocss/reset": "^66.4.1",
    "alova": "^3.3.4",
    "elegant-router": "1.0.4-beta.10",
    "element-plus": "^2.10.5",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "vue": "^3.5.17",
    "vue-router": "4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^24.2.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "czg": "^1.12.0",
    "eslint": "^9.34.0",
    "eslint-config-ali": "^16.5.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prettier-config-ali": "^1.5.0",
    "rimraf": "^6.0.1",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "stylelint": "^16.23.1",
    "stylelint-config-ali": "^2.3.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "~5.8.3",
    "unocss": "^66.4.1",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.0.4",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^2.2.12"
  }
}
